{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "connection": "Server=127.0.0.1;Port=3306;Database=YOUR_DATABASE;User=YOUR_USER;Password=YOUR_PASSWORD;"
  },
  "JWT": {
    "Audience": "user",
    "Issuer": "https://localhost:7178",
    "AccessTokenSecret": "YOUR_ACCESS_TOKEN_SECRET_AT_LEAST_32_CHARS",
    "RefreshTokenSecret": "YOUR_REFRESH_TOKEN_SECRET_AT_LEAST_32_CHARS",
    "AccessTokenExpireMinutes": 3600,
    "RefreshTokenExpireMinutes": 3600
  },
  "MailSettings": {
    "Mail": "your-email@gmail.com",
    "DisplayName": "Your Name",
    "Password": "your-app-password",
    "Host": "smtp.gmail.com",
    "Port": 587
  },
  "AllowedOrigins": [
    "http://localhost:3000",
    "http://localhost:5173",
    "https://localhost:5260",
    "exp://192.168.1.11:8081"
  ],
  "Cloudinary": {
    "CloudName": "YOUR_CLOUD_NAME",
    "ApiKey": "YOUR_API_KEY",
    "ApiSecret": "YOUR_API_SECRET"
  },
  "Vnpay": {
    "TmnCode": "YOUR_TMN_CODE",
    "HashSecret": "YOUR_HASH_SECRET",
    "BaseUrl": "https://sandbox.vnpayment.vn/paymentv2/vpcpay.html",
    "CallbackUrl": "http://localhost:5173/payment-info"
  },
  "AllowedHosts": "*"
}
